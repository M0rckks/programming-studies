algoritmo "exercicio1"
var

   A, B, C : inteiro

inicio

      //entrada
      escreva("Digite um número: ")
      leia(A)
      
      escreva("Digite um número: ")
      leia(B)
      
      //processamento
      
      C <- A + B
      
      //saída
      
      escreva(C)

fimalgoritmo